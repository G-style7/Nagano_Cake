 <div class ="container">
   <div class ="row" >


    <h1>商品一覧</h1>
    <table class = "table" >
     <div class "col">
      <thead>
       <tr>
        <th>商品ID</th>
        <th>商品名</th>
        <th>税抜価格</th>
        <th>ジャンル</th>
        <th>ステータス</th>
       </tr>
      </thead>
      <tbody>
       <% @item.each do |item| %>
       <tr>
        <td><%= item.id %></td>
        <td><%= item.name %></td>
        <td><%= item.price %></td>
        <td><%= item.genre_id %></td>
        <td><%= item.is_active %></td>
       </tr>
       <% end %>

      </tbody>
     </div>
    </table>
   </div>
  </div>
