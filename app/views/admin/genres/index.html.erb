 <div class = "container">
  <div class = "row" >
    <div class = "col-auto">
    <h1>ジャンル一覧・追加</h1>
     <%= form_with model: @genre, url: admin_genres_path ,method: :post do |f| %>
     <%= f.label "ジャンル名" %>
     <%= f.text_field :name ,class: "form-control genre_name", :placeholder => "ジャンル名" %><br>
     <%= button_to "新規登録" ,admin_genres_path, method: :post ,class: "btn btn-success" %>
     <% end %>
   </div>
  </div>
 </div>
    <h2>ジャンル</h2>
     <table class='table'>
      <tr>
       <td>
         <%= @genre.name %>. each?
       </td>
      </tr>